{
  "extends": ["eslint:recommended", "standard", "plugin:import/recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/no-named-as-default-member": "off",
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ],
    "sort-keys": "warn"
  }
}
